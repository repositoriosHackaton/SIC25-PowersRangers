{
  "name": "my-app",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "start": "react-scripts start",
    "start-api": "cd api && python app.py --no-debugger",
    "build": "vite build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "dev": "vite",
    "lint": "eslint .",
    "preview": "vite preview",
