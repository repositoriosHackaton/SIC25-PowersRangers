    "start-all": "concurrently \"npm run start\" \"npm run start-api\""
  },
  "dependencies": {
    "@vitejs/plugin-react": "^1.0.0",
    "lucide-react": "^0.344.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "^5.0.1",
    "vite": "^2.9.18"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@types/node": "^22.13.8",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.18",
    "concurrently": "^9.1.2",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "typescript": "^4.4.3",
    "typescript-eslint": "^8.3.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
